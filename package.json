  "keywords": [
    "Discord",
    "Discord Bot",
    "Discord.js",
    "typescript"
  ],
  "author": "Lynnux",
  "license": "BSD-4-Clause",
  "dependencies": {
    "@dotenvx/dotenvx": "^1.48.4",
    "@types/crypto-js": "^4.2.2",
    "crypto-js": "^4.2.0",
    "discord.js": "14.21.0",
    "kysely": "^0.28.2",
    "module-alias": "^2.2.3",
    "playwright": "^1.55.0",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.0.13",
    "better-sqlite3": "^12.2.0",
    "dotenv": "^17.2.0",
    "module-alias": "^2.2.3",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "winston": "^3.17.0"
  },
  "_moduleAliases": {
    "@": "dist/"
